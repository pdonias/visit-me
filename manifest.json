{
  "manifest_version": 2,

  "name": "Visit me",
  "description": "",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.svg",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.leboncoin.fr/*"],
      "js": ["index.js"]
    }
  ]
}
